{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "error",
//    "import/no-named-as-default":0,
    "eslint-plugin-import": true,
    "linebreak-style": 0,
    "function-paren-newline": ["error", "consistent"],
    "object-curly-newline": ["error", { "consistent": true }]
  }
}